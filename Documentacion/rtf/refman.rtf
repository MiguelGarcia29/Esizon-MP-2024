00003 {\cf18 char} *id_generator_pedido (Pedido *pedidos, {\cf18 int} tamanio_vector) \{\par
00004 \par
00005     {\cf18 int} id_generada = 1;\par
00006 \par
00007     {\cf19 if} (tamanio_vector != 0)\par
00008     \{ {\cf20 // Si no hay nada en el vector, la primer id es 0000011.}\par
00009         id_generada = atoi(pedidos[tamanio_vector - 1].id_pedido);\par
00010         id_generada++;\par
00011     \}\par
00012     {\cf18 char} *{\cf18 id} = malloc(8 * {\cf17 sizeof}({\cf18 char}));\par
00013     {\cf19 if} ({\cf18 id} == NULL)\par
00014     \{\par
00015         printf({\cf22 "Error al guardar memoria\\n"});\par
00016         exit(EXIT_FAILURE);\par
00017     \}\par
00018 \par
00019     sprintf({\cf18 id}, {\cf22 "%07d"}, id_generada);\par
00020     {\cf19 return} id;\par
00021 \}\par
00022 \par
00023 \par
00024 {\cf20 // Guarda el vector de ComportamientoLocker en el archivo siguiendo la estructura:}\par
00025 {\cf20 /*}\par
00026 {\cf20     o Identificador del pedido (Id_pedido), 7 d\'EDgitos.}\par
00027 {\cf20     o Fecha del pedido (d\'EDa, mes y a\'F1o)}\par
00028 {\cf20     o Identificador del cliente que realiza el pedido (Id_cliente), 7 d\'EDgitos.}\par
00029 {\cf20     o Lugar de entrega (Lugar): \'ABdomicilio\'BB o \'ABlocker\'BB}\par
00030 {\cf20     o Identificador de Locker (Id_locker), 10 caracteres m\'E1ximo.}\par
00031 {\cf20     o Identificador del c\'F3digo promocional o cheque regalo (Id_cod), 10 caracteres m\'E1ximo.}\par
00032 {\cf20 */}\par
00033 {\cf18 void} guardarPedidoEnArchivo(Pedido *pedidos, {\cf18 int} numPedidos)\par
00034 \{\par
00035     FILE *archivo = fopen(Pedido_txt, {\cf22 "w"});\par
00036     {\cf19 if} (archivo == NULL)\par
00037     \{\par
00038         printf({\cf22 "Error al abrir el archivo Pedidos.txt.\\n"});\par
00039         {\cf19 return};\par
00040     \}\par
00041 \par
00042     {\cf19 for} ({\cf18 int} i = 0; i < numPedidos; i++)\par
00043     \{\par
00044 \par
00045         {\cf20 // PARA QUE INTRODUZCA UN ESPACIO SI NO HAY LOCKER}\par
00046         {\cf19 if} (strcmp(pedidos[i].lugar_entrega, {\cf22 "locker"}) != 0)\par
00047         \{\par
00048             strcpy(pedidos[i].id_locker, {\cf22 " "});\par
00049         \}\par
00050 \par
00051         fprintf(archivo, {\cf22 "%s-%s-%s-%s-%s-%s-\\n"},\par
00052                 pedidos[i].id_pedido,\par
00053                 pedidos[i].fecha_pedido,\par
00054                 pedidos[i].id_cliente,\par
00055                 pedidos[i].lugar_entrega,\par
00056                 pedidos[i].id_locker,\par
00057                 pedidos[i].id_cod);\par
00058     \}\par
00059 \par
00060     fclose(archivo);\par
00061 \}\par
00062 \par
00063 Pedido *iniciarPedidosDeArchivo({\cf18 int} *numPedidos)\par
00064 \{\par
00065     FILE *archivo = fopen(Pedido_txt, {\cf22 "r"});\par
00066     {\cf19 if} (archivo == NULL)\par
00067     \{\par
00068         printf({\cf22 "Error al abrir el archivo %s.\\n"}, Pedido_txt);\par
00069         {\cf19 return} NULL;\par
00070     \}\par
00071 \par
00072     {\cf20 // Contar la cantidad de lineas en el archivo}\par
00073     {\cf18 int} count = 0;\par
00074     {\cf18 char} buffer[TAMANIO_MAXIMO_LINEA]; {\cf20 // Longitud maxima de linea}\par
00075     {\cf19 while} (fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\par
00076     \{\par
00077         count++;\par
00078     \}\par
00079 \par
00080     {\cf20 // Regresar al inicio del archivo}\par
00081     rewind(archivo);\par
00082 \par
00083     {\cf20 // Crear el vector de Locker}\par
00084     Pedido *pedidos = (Pedido *)malloc(count * {\cf17 sizeof}(Pedido));\par
00085     {\cf19 if} (pedidos == NULL)\par
00086     \{\par
00087         fclose(archivo);\par
00088         printf({\cf22 "Error: No se pudo asignar memoria para el vector de Pedido.\\n"});\par
00089         {\cf19 return} NULL;\par
00090     \}\par
00091 \par
00092     {\cf20 // Leo cada linea y rellenar el vector de adminProv}\par
00093     {\cf19 for}({\cf18 int} i=0;i<count;i++)\par
00094     \{\par
00095         {\cf19 if}(fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\{\par
00096         {\cf18 char} *token = strtok(buffer, {\cf22 "-"});\par
00097         strncpy(pedidos[i].id_pedido, token, {\cf17 sizeof}(pedidos[i].id_pedido));\par
00098         token = strtok(NULL, {\cf22 "-"});\par
00099         strncpy(pedidos[i].fecha_pedido, token, {\cf17 sizeof}(pedidos[i].fecha_pedido));\par
00100         token = strtok(NULL, {\cf22 "-"});\par
00101         strncpy(pedidos[i].id_cliente, token, {\cf17 sizeof}(pedidos[i].id_cliente));\par
00102         token = strtok(NULL, {\cf22 "-"});\par
00103         strncpy(pedidos[i].lugar_entrega, token, {\cf17 sizeof}(pedidos[i].lugar_entrega));\par
00104         token = strtok(NULL, {\cf22 "-"});\par
00105         {\cf20 // SI NO ESTA EN LOCKER QUE DEJE ID LOCKER VACIA}\par
00106         {\cf19 if} (strcmp(pedidos[i].lugar_entrega, {\cf22 "locker"}) != 0)\par
00107             strncpy(pedidos[i].id_locker, {\cf22 ""}, {\cf17 sizeof}(pedidos[i].id_locker));\par
00108         {\cf19 else}\par
00109             strncpy(pedidos[i].id_locker, token, {\cf17 sizeof}(pedidos[i].id_locker));\par
00110         token = strtok(NULL, {\cf22 "-"});\par
00111         strncpy(pedidos[i].id_cod, token, {\cf17 sizeof}(pedidos[i].id_cod));\par
00112 \par
00113         \}\par
00114     \}\par
00115 \par
00116     fclose(archivo);\par
00117     *numPedidos = count;\par
00118     {\cf19 return} pedidos;\par
00119 \}\par
00120 \par
00121 {\cf18 void} baja_pedido(Pedido **pedidos, {\cf18 int} *npedidos, {\cf18 char} *id_pedido_baja) \{\par
00122     {\cf18 int} i, j;\par
00123     {\cf19 for} (i = 0; i < *npedidos; i++) \{\par
00124         {\cf19 if} (strcmp((*pedidos)[i].id_pedido, id_pedido_baja) == 0) \{\par
00125             {\cf20 // Si se encuentra el pedido, se eliminan los datos de esa posici\'F3n}\par
00126             {\cf19 for} (j = i; j < *npedidos - 1; j++) \{\par
00127                 (*pedidos)[j] = (*pedidos)[j + 1];\par
00128             \}\par
00129             (*npedidos)--; {\cf20 // Se reduce la cantidad de pedidos}\par
00130             printf({\cf22 "Pedido dado de baja.\\n"} );\par
00131             {\cf19 return};\par
00132         \}\par
00133     \}\par
00134     printf({\cf22 "No se encontro ningun pedido con el ID.\\n"});\par
00135 \}\par
