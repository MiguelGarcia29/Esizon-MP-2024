00230 Devolucion *iniciarDevolucionDeArchivo({\cf18 int} *numDevolucion)\par
00231 \{\par
00232     FILE *archivo = fopen(Devoluciones_txt, {\cf22 "r"});\par
00233     {\cf19 if} (archivo == NULL)\par
00234     \{\par
00235         printf({\cf22 "Error al abrir el archivo %s.\\n"}, Devoluciones_txt);\par
