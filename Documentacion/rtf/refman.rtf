00059 {\cf18 int} ocupados_lockers(Locker *l, {\cf18 int} tamanio, {\cf18 char} *{\cf18 id}) \{\par
00060 \par
00061     {\cf19 for} ({\cf18 int} i = 0; i < tamanio; i++) \{\par
00062         {\cf19 if} (strcmp({\cf18 id}, l[i].id_locker) == 0) \{ {\cf20 // Compara el ID dado con el ID del locker en la posici\'F3n i}\par
00063             {\cf19 if} (l[i].num_compt > l[i].num_compocup) \{ {\cf20 // Verifica si hay compartimentos disponibles}\par
00064                 l[i].num_compocup++; {\cf20 // Incrementa el contador de compartimentos ocupados}\par
00065                 printf({\cf22 "Se ha a\'F1adido correctamente\\n"});\par
00066                 {\cf19 return} 0; {\cf20 // Devuelve 0 si no est\'E1 lleno}\par
00067             \} {\cf19 else} \{\par
00068                 printf({\cf22 "El locker est\'E1 lleno\\n"});\par
00069                 {\cf19 return} 1; {\cf20 // Devuelve 1 si est\'E1 lleno}\par
00070             \}\par
00071         \}\par
00072     \}\par
