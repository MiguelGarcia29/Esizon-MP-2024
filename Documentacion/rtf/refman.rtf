00156                 flushInputBuffer();\par
00157 \par
00158                 {\cf19 if} (opcion == 1)\par
00159                     strcpy(desc[i].estado, {\cf22 "activo"});\par
00160                 {\cf19 else} {\cf19 if} (opcion == 2)\par
00161                     strcpy(desc[i].estado, {\cf22 "inactivo"});\par
00162                 {\cf19 else}\par
00163                     printf({\cf22 "\\nOpcion invalida\\n"});\par
00164             {\cf19 break};\par
00165 \par
00166             {\cf19 case} 2:\par
00167                 printf({\cf22 "\\nNuevo importe: "});\par
00168                 scanf({\cf22 "%f"}, &desc[i].importe);\par
00169             {\cf19 break};\par
00170 \par
00171             {\cf19 default}:\par
00172                 printf({\cf22 "\\nOpcion invalida\\n"});\par
00173             {\cf19 break};\par
00174         \}\par
00175     \} {\cf19 else} \{\par
00176         printf({\cf22 "Descuento no encontrado\\n"});\par
00177     \}\par
00178 \}\par
00179 \par
00180 {\cf20 // Funci\'F3n para encontrar un descuento por su ID}\par
00181 {\cf18 int} encontrar_descuento({\cf18 int} cantdad_desc, Descuento *desc, {\cf18 char} *{\cf18 id}) \{\par
00182     {\cf18 int} i, encontrado = 0;\par
00183 \par
00184     {\cf19 for} (i = 0; i < cantdad_desc && encontrado == 0; i++) \{\par
00185         {\cf19 if} (strcmp(desc[i].id_cod, {\cf18 id}) == 0) \{\par
00186             encontrado = 1;\par
00187         \}\par
00188     \}\par
00189     {\cf19 if} (encontrado == 0) \{\par
00190         i = 0;\par
00191     \}\par
00192 \par
00193     {\cf19 return} i - 1;\par
00194 \}\par
00195 \par
00196 \par
00197 {\cf20 // Guarda el vector de Descuentos en el archivo siguiendo la estructura:}\par
00198 {\cf20 /*}\par
00199 {\cf20     o Identificador del c\'F3digo promocional o cheque regalo (Id_cod), 10 caracteres m\'E1ximo.}\par
00200 {\cf20     o Descripci\'F3n del descuento (Descrip), 50 caracteres m\'E1ximo.}\par
00201 {\cf20     o Tipo de descuento (Tipo): \'ABcodpro\'BB (c\'F3digo promocional) o \'ABcheqreg\'BB (cheque regalo)}\par
00202 {\cf20     o Estado (Estado): \'ABactivo\'BB o \'ABinactivo\'BB}\par
00203 {\cf20     o Importe del descuento en euros (Importe)}\par
00204 {\cf20     o Aplicabilidad (Aplicable): \'ABtodos\'BB (si es aplicable a todos los productos) o \'ABesizon\'BB (si s\'F3lo es}\par
00205 {\cf20     aplicable a los gestionados por ESIZON.}\par
00206 {\cf20 */}\par
00207 \par
00208 {\cf18 void} guardarDescuentosEnArchivo(Descuento *descuentos, {\cf18 int} numDescuentos)\par
00209 \{\par
00210     FILE *archivo = fopen(Descuento_txt, {\cf22 "w"});\par
00211     {\cf19 if} (archivo == NULL)\par
00212     \{\par
00213         printf({\cf22 "Error al abrir el archivo Descuentos.txt.\\n"});\par
00214         {\cf19 return};\par
00215     \}\par
00216 \par
00217     {\cf19 for} ({\cf18 int} i = 0; i < numDescuentos; i++)\par
00218     \{\par
00219         fprintf(archivo, {\cf22 "%s-%s-%s-%s-%.2f-%s-\\n"},\par
00220                 descuentos[i].id_cod,\par
00221                 descuentos[i].descrip,\par
00222                 descuentos[i].tipo,\par
00223                 descuentos[i].estado,\par
00224                 descuentos[i].importe,\par
00225                 descuentos[i].aplicable);\par
00226     \}\par
00227 \par
00228     fclose(archivo);\par
00229 \}\par
00230 \par
00231 Descuento *iniciarDescuentosDeArchivo({\cf18 int} *numDesc)\par
00232 \{\par
00233     FILE *archivo = fopen(Descuento_txt, {\cf22 "r"});\par
00234     {\cf19 if} (archivo == NULL)\par
00235     \{\par
00236         printf({\cf22 "Error al abrir el archivo %s.\\n"}, Descuento_txt);\par
00237         {\cf19 return} NULL;\par
00238     \}\par
00239 \par
00240     {\cf20 // Contar la cantidad de lineas en el archivo}\par
00241     {\cf18 int} count = 0;\par
00242     {\cf18 char} buffer[TAMANIO_MAXIMO_LINEA]; {\cf20 // Longitud maxima de linea}\par
00243     {\cf19 while} (fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\par
00244     \{\par
00245         count++;\par
00246     \}\par
00247 \par
00248     {\cf20 // Regresar al inicio del archivo}\par
00249     rewind(archivo);\par
00250 \par
00251     {\cf20 // Crear el vector de descuentos}\par
00252     Descuento *descuentos =(Descuento *) malloc(count * {\cf17 sizeof}(Descuento));\par
00253     {\cf19 if} (descuentos == NULL)\par
00254     \{\par
00255         fclose(archivo);\par
00256         printf({\cf22 "Error: No se pudo asignar memoria para el vector de descuentos.\\n"});\par
00257         {\cf19 return} NULL;\par
00258     \}\par
00259 \par
00260     {\cf20 // Leo cada linea y rellenar el vector de adminProv}\par
00261     {\cf19 for}({\cf18 int} i=0;i<count;i++)\par
00262     \{\par
00263         {\cf19 if}(fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\{\par
00264         {\cf18 char} *token = strtok(buffer, {\cf22 "-"});\par
00265         strncpy(descuentos[i].id_cod, token, {\cf17 sizeof}(descuentos[i].id_cod));\par
00266         token = strtok(NULL, {\cf22 "-"});\par
00267         strncpy(descuentos[i].descrip, token, {\cf17 sizeof}(descuentos[i].descrip));\par
00268         token = strtok(NULL, {\cf22 "-"});\par
00269         strncpy(descuentos[i].tipo, token, {\cf17 sizeof}(descuentos[i].tipo));\par
00270         token = strtok(NULL, {\cf22 "-"});\par
00271         strncpy(descuentos[i].estado, token, {\cf17 sizeof}(descuentos[i].estado));\par
00272         token = strtok(NULL, {\cf22 "-"});\par
00273         descuentos[i].importe = atof(token);\par
00274         token = strtok(NULL, {\cf22 "-"});\par
00275         strncpy(descuentos[i].aplicable, token, {\cf17 sizeof}(descuentos[i].aplicable));\par
00276         \}\par
00277     \}\par
00278 \par
00279     fclose(archivo);\par
00280     *numDesc = count;\par
00281     {\cf19 return} descuentos;\par
00282 \}\par
00283 \par
00284 {\cf18 void} listar_descuentos(Descuento **descuentos, {\cf18 int} *tamano) \{\par
00285     printf({\cf22 "Lista de descuentos:\\n"});\par
00286     {\cf19 for} ({\cf18 int} i = 0; i < *tamano; i++) \{\par
00287         printf({\cf22 "Descuento %d:\\n"}, i + 1);\par
00288         printf({\cf22 "ID C\'F3digo: %s\\n"}, (*descuentos)[i].id_cod);\par
00289         printf({\cf22 "Descripci\'F3n: %s\\n"}, (*descuentos)[i].descrip);\par
00290         printf({\cf22 "Tipo: %s\\n"}, (*descuentos)[i].tipo);\par
00291         printf({\cf22 "Estado: %s\\n"}, (*descuentos)[i].estado);\par
00292         printf({\cf22 "Importe: %.2f\\n"}, (*descuentos)[i].importe);\par
00293         printf({\cf22 "Aplicable: %s\\n"}, (*descuentos)[i].aplicable);\par
00294         printf({\cf22 "\\n"});\par
00295     \}\par
00296 \}\par
