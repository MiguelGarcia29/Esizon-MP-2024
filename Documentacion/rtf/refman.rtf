00003 {\cf20 // Funci\'F3n para obtener la fecha de caducidad de un compartimento}\par
00004 {\cf18 void} fecha_caducidad_compartimento({\cf18 char} *fecha) \{\par
00005     time_t rawtime;\par
00006     {\cf17 struct }tm *info;\par
00007 \par
00008     {\cf20 // Obtener la fecha y hora actual}\par
00009     time(&rawtime);\par
00010     rawtime += 7 * 24 * 60 * 60; {\cf20 // 7 d\'EDas en segundos}\par
00011 \par
00012     info = localtime(&rawtime);\par
00013     strftime(fecha, 11, {\cf22 "%d/%m/%Y"}, info);\par
00014 \}\par
00015 \par
00016 {\cf20 // Funci\'F3n para dar de alta un nuevo compartimento}\par
00017 {\cf18 void} alta_comportimiento(CompartimentoLocker **l, {\cf18 int}* tamanio_vector) \{\par
00018     CompartimentoLocker nuevo_compartimento;\par
00019     {\cf18 char} fecha[11];\par
00020     obtener_fecha_actual(fecha);\par
00021     strcpy(nuevo_compartimento.fecha_ocupacion, fecha);\par
00022 \par
00023     {\cf18 char} fechacaducidad[11];\par
00024     fecha_caducidad_compartimento(fechacaducidad);\par
00025     strcpy(nuevo_compartimento.fecha_caducidad, fechacaducidad);\par
00026 \par
00027     strcpy(nuevo_compartimento.estado, {\cf22 "ocupado"});\par
00028 \par
00029     {\cf20 // Realiza una realocaci\'F3n de memoria para agregar el nuevo compartimento al arreglo}\par
00030     *l = (CompartimentoLocker *)realloc(*l, (*tamanio_vector + 1) * {\cf17 sizeof}(CompartimentoLocker));\par
00031     {\cf19 if} (*l == NULL) \{\par
00032         printf({\cf22 "Error al asignar memoria para el nuevo compartimento. \\n"});\par
00033         exit(EXIT_FAILURE);\par
00034     \}\par
00035     (*l)[*tamanio_vector] = nuevo_compartimento; {\cf20 // Guarda el nuevo compartimento en el arreglo}\par
00036     (*tamanio_vector)++;\par
00037 \}\par
00038 \par
00039 {\cf20 // Funci\'F3n para dar de baja un compartimiento}\par
00040 {\cf18 void} baja_comportamiento(CompartimentoLocker *l, {\cf18 int}* tamanio, {\cf18 char} *id_baja, {\cf18 char} *codigo_de_baja) \{\par
00041     {\cf18 int} encontrado = 0;\par
00042 \par
00043     {\cf20 // Busca el compartimento con el ID y c\'F3digo dados y lo elimina del arreglo}\par
00044     {\cf19 for} ({\cf18 int} i = 0; i < *tamanio; i++) \{\par
00045         {\cf19 if} (strcmp(l[i].id_locker, id_baja) == 0 && strcmp(l[i].cod_locker, codigo_de_baja) == 0) \{\par
00046             encontrado = 1;\par
00047 \par
00048             {\cf20 // Reorganiza el arreglo para eliminar el compartimento encontrado}\par
00049             {\cf19 for} ({\cf18 int} j = i; j < *tamanio - 1; j++) \{\par
00050                 l[j] = l[j + 1];\par
00051             \}\par
00052 \par
00053             (*tamanio)--; {\cf20 // Reduce el tama\'F1o del arreglo}\par
00054             printf({\cf22 "Compartimento ha sido dado de baja correctamente.\\n"});\par
00055             {\cf19 break}; {\cf20 // Sale del bucle una vez encontrado y eliminado el compartimento}\par
00056         \}\par
00057     \}\par
00058 \par
00059     {\cf19 if} (encontrado == 0) \{\par
00060         printf({\cf22 "La ID registrada no ha sido encontrada.\\n"});\par
00061     \}\par
00062 \}\par
00063 \par
00064 {\cf20 // Funci\'F3n para mostrar un listado de compartimientos}\par
00065 {\cf18 void} listado_comportamiento(CompartimentoLocker *l, {\cf18 int} tamanio) \{\par
00066     {\cf20 // Recorre el arreglo de compartimientos y muestra la informaci\'F3n de cada uno}\par
00067     {\cf19 for} ({\cf18 int} i = 0; i < tamanio; i++) \{\par
00068         printf({\cf22 "%s-%d-%s-%s-%s-%s\\n"}, l[i].id_locker, l[i].num_comp, l[i].cod_locker, l[i].estado, l[i].fecha_ocupacion,l[i].fecha_caducidad );\par
00069     \}\par
00070 \}\par
00071 \par
00072 \par
00073 {\cf20 // Guarda el vector de ComportamientoLocker en el archivo siguiendo la estructura:}\par
00074 {\cf20 /*}\par
00075 {\cf20     o Identificador del locker (Id_locker), 10 caracteres m\'E1ximo.}\par
00076 {\cf20     o N\'FAmero de compartimento (Num_comp).}\par
00077 {\cf20     o C\'F3digo del locker (Cod_locker).}\par
00078 {\cf20     o Estado: \'ABocupado\'BB o \'ABvac\'EDo\'BB}\par
00079 {\cf20     o Fecha ocupaci\'F3n (d\'EDa, mes y a\'F1o).}\par
00080 {\cf20     o Fecha caducidad (d\'EDa, mes y a\'F1o).}\par
00081 {\cf20 */}\par
00082 {\cf18 void} guardarCompartimentoLockerEnArchivo(CompartimentoLocker *comportamientos, {\cf18 int} numComportamientos)\par
00083 \{\par
00084     FILE *archivo = fopen(CompartimentoLocker_txt, {\cf22 "w"});\par
00085     {\cf19 if} (archivo == NULL)\par
00086     \{\par
00087         printf({\cf22 "Error al abrir el archivo CompartimentosLockers.txt.\\n"});\par
00088         {\cf19 return};\par
00089     \}\par
00090 \par
00091     {\cf19 for} ({\cf18 int} i = 0; i < numComportamientos; i++)\par
00092     \{\par
