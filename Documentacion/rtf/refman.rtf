00003 {\cf20 // Funci\'F3n para generar un ID \'FAnico para un nuevo transportista}\par
00004 {\cf18 char}* id_generator_trans(Transportista *t, {\cf18 int} tamanio_vector) \{\par
00005     {\cf18 int} id_generada = 1;\par
00006 \par
00007     {\cf19 if} (tamanio_vector != 0) \{\par
00008         id_generada = atoi(t[tamanio_vector - 1].id_transp) + 1;\par
00009     \}\par
00010 \par
00011     {\cf18 char}* {\cf18 id} = malloc(5 * {\cf17 sizeof}({\cf18 char})); {\cf20 // Se asigna memoria para el ID generado}\par
00012     {\cf19 if} ({\cf18 id} == NULL) \{\par
00013         printf({\cf22 "Error al asignar memoria. \\n"});\par
00014         exit(EXIT_FAILURE);\par
00015     \}\par
00016 \par
00017     sprintf({\cf18 id}, {\cf22 "%04d"}, id_generada); {\cf20 // Se genera el ID con formato de 4 d\'EDgitos}\par
00018 \par
00019     {\cf19 return} id;\par
00020 \}\par
00021 \par
00022 {\cf20 // Funci\'F3n para dar de alta un nuevo transportista}\par
00023 {\cf18 void} alta_transportista(Transportista **t, {\cf18 int}* tamanio_vector)\{\par
00024 \par
00025     Transportista nuevo_transportista;\par
00026     {\cf18 char} *cadena = id_generator_trans(*t, *tamanio_vector); {\cf20 // Genera un nuevo ID para el transportista}\par
00027     strcpy(nuevo_transportista.id_transp, cadena); {\cf20 // Asigna el ID generado al nuevo transportista}\par
00028     free(cadena); {\cf20 // Libera la memoria asignada para el ID generado}\par
00029 \par
00030     {\cf20 // Solicita al usuario informaci\'F3n sobre el nuevo transportista}\par
00031     printf({\cf22 "\\nDime el nombre:"});\par
00032     scanf({\cf22 "%20s"}, nuevo_transportista.nombre);\par
00033     flushInputBuffer();\par
00034 \par
00035     printf({\cf22 "\\nDime el correo:"});\par
00036     scanf({\cf22 "%30s"}, nuevo_transportista.email);\par
00037     flushInputBuffer();\par
00038 \par
00039     printf({\cf22 "\\nDime la contrase\'F1a para acceder al sistema:"});\par
00040     scanf({\cf22 "%15s"}, nuevo_transportista.contrasenia);\par
00041     flushInputBuffer();\par
00042 \par
00043     printf({\cf22 "\\nDime el nombre de la empresa:"});\par
00044     scanf({\cf22 "%20s"}, nuevo_transportista.nombre_empresa);\par
00045     flushInputBuffer();\par
00046 \par
00047     printf({\cf22 "\\nDime la ciudad de reparto:"});\par
00048     scanf({\cf22 "%20s"}, nuevo_transportista.ciudad_reparto);\par
00049     flushInputBuffer();\par
00050 \par
00051     *t = (Transportista *)realloc(*t, (*tamanio_vector + 1) * {\cf17 sizeof}(Transportista));\par
00052     {\cf19 if} (*t == NULL) \{\par
00053         printf({\cf22 "Error al asignar memoria. \\n"});\par
00054         exit(EXIT_FAILURE);\par
00055     \}\par
00056     (*t)[*tamanio_vector] = nuevo_transportista; {\cf20 // Guarda el nuevo transportista en el arreglo}\par
00057     (*tamanio_vector)++;\par
