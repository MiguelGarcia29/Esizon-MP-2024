00119     printf({\cf22 "Ingrese el ID de la empresa: "});\par
00120     fgets(nuevoProveedor.id_empresa, 5, stdin);\par
00121     nuevoProveedor.id_empresa[strcspn(nuevoProveedor.id_empresa, {\cf22 "\\n"})] = {\cf23 '\\0'};\par
00122 \par
