00470             printf({\cf22 "No hay productos asociados a esta categor\'EDa.\\n"});\par
00471         \}\par
00472     \}\par
00473 \}\par
00474 \par
00475 Producto* agregar_a_cesta(Producto **catalogo, {\cf18 int} *num_productos, {\cf18 float} *coste, {\cf18 int} *tamLista) \{\par
00476     {\cf18 int} capacidad = 1;\par
00477     {\cf18 int} num_seleccionados = 0;\par
00478     Producto *cesta = malloc(capacidad * {\cf17 sizeof}(Producto));\par
00479     {\cf18 float} precio = 0;\par
00480 \par
00481     {\cf18 char} nombre_producto[16];\par
00482     {\cf19 while} (1) \{\par
00483         printf({\cf22 "Ingrese el nombre del producto que desea agregar a la cesta (o escriba 'fin' para salir): "});\par
00484         fgets(nombre_producto, 16, stdin);\par
00485         nombre_producto[strcspn(nombre_producto, {\cf22 "\\n"})] = {\cf23 '\\0'};\par
00486         {\cf19 if} (strcmp(nombre_producto, {\cf22 "fin"}) == 0) \{\par
00487             {\cf19 break};\par
00488         \}\par
00489 \par
00490         {\cf20 // Buscar el producto en el cat\'E1logo}\par
00491         {\cf18 int} i;\par
00492         {\cf19 for} (i = 0; i < *num_productos; i++) \{\par
00493             {\cf19 if} (strcmp((*catalogo)[i].nombre, nombre_producto) == 0) \{\par
00494                 {\cf20 // Agregar el producto a la cesta}\par
00495                 {\cf19 if} (num_seleccionados == capacidad) \{\par
00496                     {\cf20 // Aumentar la capacidad de la cesta si es necesario}\par
00497                     capacidad += 1;\par
00498                     cesta = realloc(cesta, capacidad * {\cf17 sizeof}(Producto));\par
00499                 \}\par
00500                 cesta[num_seleccionados] = (*catalogo)[i];\par
00501                 num_seleccionados++;\par
00502                 precio += (*catalogo)[i].importe;\par
00503                 {\cf19 break};\par
00504             \}\par
00505         \}\par
00506     \}\par
00507 \par
00508     capacidad--;\par
00509     *tamLista = capacidad;\par
00510     cesta = realloc(cesta, capacidad * {\cf17 sizeof}(Producto));\par
00511     *coste = precio;\par
00512 \par
00513     {\cf19 return} cesta;\par
00514 \}\par
}
