00244         count++;\par
00245     \}\par
00246 \par
00247     {\cf20 // Regresar al inicio del archivo}\par
00248     rewind(archivo);\par
00249 \par
00250     {\cf20 // Crear el vector de Devoluciones}\par
00251     Devolucion *devoluciones =(Devolucion *) malloc(count * {\cf17 sizeof}(Devolucion));\par
00252     {\cf19 if} (devoluciones == NULL)\par
00253     \{\par
00254         fclose(archivo);\par
00255         printf({\cf22 "Error: No se pudo asignar memoria para el vector de Devoluciones.\\n"});\par
00256         {\cf19 return} NULL;\par
