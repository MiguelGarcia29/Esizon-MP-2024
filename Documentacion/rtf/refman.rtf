00206     {\cf19 for} ({\cf18 int} i = 0; i < cant_desc_cli && encontrado == 0; i++) \{\par
00207         {\cf19 if} (strcmp(desc_cliente[i].id_cliente, id_cliente) == 0 && strcmp(desc_cliente[i].id_cod, id_descuento) == 0) \{\par
00208             desc_cliente[i].estado = 0; {\cf20 // Establecer el estado como no usado}\par
00209             printf({\cf22 "El descuento %s para el cliente %s ha sido marcado como no usado.\\n"}, id_descuento, id_cliente);\par
00210             encontrado = 1;\par
00211         \}\par
00212     \}\par
00213     {\cf20 // Si no se encuentra el descuento para el cliente especificado}\par
00214     {\cf19 if}(encontrado == 0) \{\par
00215         printf({\cf22 "No se encontr\'F3 el descuento %s para el cliente %s.\\n"}, id_descuento, id_cliente);\par
00216     \}\par
00217 \}\par
00218 \par
00219 {\cf20 // Funci\'F3n para aplicar el descuento al precio}\par
00220 {\cf18 void} aplicar_descuento({\cf18 float} *precio, Descuento *desc, DescuentoCliente *desccli, {\cf18 char} id_cliente[8], Producto *prod, {\cf18 int} cantdad_desc, {\cf18 int} cantdad_desccli, {\cf18 int} numprodpe) \{\par
00221     {\cf18 int} a, i, j, encontrado = 0;\par
00222     {\cf18 char} id_desc[11];\par
00223     Descuento elegido;\par
00224     DescuentoCliente elegidocliente;\par
00225 \par
00226     printf({\cf22 "\\n\\nUtilizar un codigo de descuento\\n\\n"});\par
00227     printf({\cf22 "(1) Si\\n"});\par
00228     printf({\cf22 "(2) En otra ocasion\\n"});\par
00229 \par
00230     flushInputBuffer();\par
00231     scanf({\cf22 "%d"}, &a);\par
00232 \par
00233     {\cf19 if}(a == 1) \{\par
00234         mostrar_descuentos_cliente(id_cliente, desc, desccli, cantdad_desc, cantdad_desccli);\par
00235         printf({\cf22 "\\n\\n\\nIntroduzca el codigo que desea utilizar: "});\par
00236         flushInputBuffer();\par
00237         scanf({\cf22 "%11s"}, id_desc);\par
00238     \} {\cf19 else} \{\par
00239         {\cf19 return};\par
00240     \}\par
00241 \par
00242     {\cf19 for}(i = 0; i < cantdad_desccli && encontrado == 0; i++) \{\par
00243         {\cf19 if}(strcmp(desccli[i].id_cod, id_desc) == 0 && strcmp(desccli[i].id_cliente, id_cliente) == 0) \{\par
00244             elegidocliente = desccli[i];\par
00245             encontrado = 1;\par
00246         \}\par
00247     \}\par
00248 \par
00249     {\cf19 if}(encontrado == 0) \{\par
00250         printf({\cf22 "\\nDescuento no encontrado\\n"});\par
00251         {\cf19 return};\par
00252     \}\par
00253 \par
00254     encontrado = 0;\par
00255 \par
00256     {\cf19 for}(i = 0; i < cantdad_desc && encontrado == 0; i++) \{\par
00257         {\cf19 if}(strcmp(id_desc, desc[i].id_cod) == 0) \{\par
00258             elegido = desc[i];\par
00259             encontrado = 1;\par
00260         \}\par
00261     \}\par
00262 \par
00263     {\cf19 if}(encontrado == 0) \{\par
00264         printf({\cf22 "\\nProducto no encontrado\\n\\n"});\par
00265         {\cf19 return};\par
00266     \}\par
00267 \par
00268     {\cf19 if}(todos_esizon(elegido, prod, numprodpe) == 0 && activo_inactivo(elegido) == 0 && aplicado(elegidocliente) == 0 && cheq_cod(elegido, numprodpe) == 0) \{\par
00269         descontar(elegido, precio);\par
00270     \} {\cf19 else} \{\par
00271         printf({\cf22 "Descuento no disponible.\\n"});\par
00272     \}\par
00273 \}\par
00274 \par
