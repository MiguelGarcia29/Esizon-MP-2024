00003 {\cf20 // Funci\'F3n para generar un ID para un administrador o proveedor}\par
00004 {\cf18 char}* id_generator(AdminProv *a, {\cf18 int} tamanio_vector) \{\par
00005     {\cf18 int} id_generada = 1;\par
00006 \par
00007     {\cf20 // Si hay elementos en el vector, obtiene el \'FAltimo ID y genera uno nuevo increment\'E1ndolo}\par
00008     {\cf19 if} (tamanio_vector != 0) \{\par
00009         id_generada = atoi(a[tamanio_vector - 1].id_empresa) + 1;\par
00010     \}\par
00011 \par
00012     {\cf20 // Asigna memoria para el ID generado}\par
00013     {\cf18 char}* {\cf18 id} = malloc(5 * {\cf17 sizeof}({\cf18 char}));\par
00014     {\cf19 if} ({\cf18 id} == NULL) \{\par
00015         printf({\cf22 "Error al asignar memoria. \\n"});\par
00016         exit(EXIT_FAILURE);\par
00017     \}\par
00018 \par
00019     {\cf20 // Formatea la nueva ID y la guarda en el arreglo}\par
00020     sprintf({\cf18 id}, {\cf22 "%03d"}, id_generada);\par
00021 \par
00022     {\cf19 return} id;\par
00023 \}\par
00024 {\cf20 // Funci\'F3n para agregar un nuevo administrador o proveedor}\par
00025 {\cf18 void} alta_AdminProv(AdminProv **l, {\cf18 int}* tamanio_vector) \{\par
00026     AdminProv nuevo_adminprov;\par
00027     {\cf20 // Genera un nuevo ID para el administrador o proveedor}\par
00028     {\cf18 char} *cadena = id_generator(*l, *tamanio_vector);\par
00029     strcpy(nuevo_adminprov.id_empresa, cadena);\par
00030     free(cadena); {\cf20 // Libera la memoria asignada para el ID generado}\par
00031 \par
00032     {\cf20 // Solicita al usuario informaci\'F3n sobre el nuevo administrador o proveedor}\par
00033     {\cf18 int} respuesta;\par
00034     printf({\cf22 "\\nSeleccione nuevo rol:"});\par
00035     printf({\cf22 "\\n1-Administrador"});\par
00036     printf({\cf22 "\\n2-Proveedor\\n"});\par
00037     scanf({\cf22 "%d"}, &respuesta);\par
00038    {\cf19 switch}(respuesta)\par
00039    \{\par
00040         {\cf19 case} 1:\par
00041             strcpy(nuevo_adminprov.nombre, {\cf22 "ESIZON"});\par
00042             strcpy(nuevo_adminprov.perfil_usuario, {\cf22 "administrador"});\par
00043             {\cf19 break};\par
00044 \par
00045         {\cf19 case} 2:\par
00046             printf({\cf22 "Dime el nombre de la empresa:\\n"});\par
00047             {\cf18 char} empresa[20];\par
00048             fgets(empresa, 20, stdin);\par
00049             empresa[strcspn(empresa, {\cf22 "\\n"})] = {\cf23 '\\0'};\par
00050             strcpy(nuevo_adminprov.nombre, empresa);\par
00051             strcpy(nuevo_adminprov.perfil_usuario, {\cf22 "proveedor"});\par
00052             {\cf19 break};\par
00053         {\cf19 default}:\par
00054             printf({\cf22 "Esa opcion es incorrecta\\n"});\par
00055    \}\par
00056 \par
00057     printf({\cf22 "\\nDime tu correo electronico:"});\par
00058     scanf({\cf22 "%30s"}, nuevo_adminprov.email);\par
00059     nuevo_adminprov.email[strcspn(nuevo_adminprov.email, {\cf22 "\\n"})] = {\cf23 '\\0'};\par
00060 \par
00061     printf({\cf22 "\\nDime tu contrasenia:"});\par
00062     scanf({\cf22 "%16s"}, nuevo_adminprov.contrasenia);\par
00063     nuevo_adminprov.contrasenia[strcspn(nuevo_adminprov.contrasenia, {\cf22 "\\n"})] = {\cf23 '\\0'};\par
00064 \par
00065     {\cf20 // Realiza una realocaci\'F3n de memoria para agregar el nuevo administrador o proveedor al arreglo}\par
00066     *l = (AdminProv *)realloc(*l, (*tamanio_vector + 1) * {\cf17 sizeof}(AdminProv));\par
00067     {\cf19 if} (*l == NULL) \{\par
00068         printf({\cf22 "Error al asignar memoria para el nuevo administrador o proveedor. \\n"});\par
00069         exit(EXIT_FAILURE);\par
00070     \}\par
00071     (*l)[*tamanio_vector] = nuevo_adminprov; {\cf20 // Guarda el nuevo administrador o proveedor en el arreglo}\par
00072     (*tamanio_vector)++;\par
00073 \}\par
00074 {\cf20 // Funci\'F3n para dar de baja un administrador o proveedor}\par
00075 {\cf18 void} baja_adminprov(AdminProv *l, {\cf18 int}* tamanio, {\cf18 char} *id_baja) \{\par
00076     {\cf18 int} encontrado = 0;\par
00077 \par
00078     {\cf20 // Busca el administrador o proveedor con el ID dado y lo elimina del arreglo}\par
00079     {\cf19 for} ({\cf18 int} i = 0; i < *tamanio; i++) \{\par
00080         {\cf19 if} (strcmp(l[i].id_empresa, id_baja) == 0) \{\par
00081             encontrado = 1;\par
00082 \par
00083             {\cf20 // Reorganiza el arreglo para eliminar el administrador o proveedor encontrado}\par
00084             {\cf19 for} ({\cf18 int} j = i; j < *tamanio - 1; j++) \{\par
00085                 l[j] = l[j + 1];\par
00086             \}\par
00087 \par
00088             (*tamanio)--; {\cf20 // Reduce el tama\'F1o del arreglo}\par
00089             printf({\cf22 "Ha sido dado de baja correctamente.\\n"}, id_baja);\par
00090             {\cf19 break}; {\cf20 // Sale del bucle una vez encontrado y eliminado el administrador o proveedor}\par
00091         \}\par
00092     \}\par
00093 \par
00094     {\cf19 if} (encontrado == 0) \{\par
00095         printf({\cf22 "La ID registrada no ha sido encontrada.\\n"});\par
00096     \}\par
00097 \}\par
00098 {\cf20 // Funci\'F3n para listar los administradores o proveedores}\par
00099 {\cf18 void} listado_adminprov(AdminProv *l, {\cf18 int} tamanio) \{\par
00100     {\cf20 // Recorre el arreglo de administradores o proveedores y muestra la informaci\'F3n de cada uno}\par
00101     {\cf19 for} ({\cf18 int} i = 0; i < tamanio; i++) \{\par
00102         printf({\cf22 "%s-%s-%s-%s-%s\\n"}, l[i].id_empresa, l[i].nombre, l[i].email, l[i].contrasenia, l[i].perfil_usuario);\par
00103     \}\par
00104 \}\par
00105 {\cf20 // Guarda el vector de AdminYProvs en el archivo siguiendo la estructura:}\par
00106 {\cf20 /*}\par
00107 {\cf20     o Identificador de la empresa administradora o proveedora (Id_empresa), 4 digitos.}\par
00108 {\cf20     o Nombre de la empresa (Nombre), seria ESIZON si es administrador, 20 caracteres maximo.}\par
00109 {\cf20     o Email (email), 30 caracteres maximo, sera usado como nombre de usuario para el acceso a la}\par
00110 {\cf20     plataforma.}\par
00111 {\cf20     o Contrasenia para acceder al sistema (Contrasenia), con 15 caracteres maximo.}\par
00112 {\cf20     o Perfil del usuario (Perfil_usuario): \'ABadministrador\'BB o \'ABproveedor\'BB.}\par
00113 {\cf20 */}\par
00114 {\cf18 void} guardarAdminProvEnArchivo(AdminProv *admins, {\cf18 int} numAdmins)\par
00115 \{\par
00116     FILE *archivo = fopen(AdminProv_txt, {\cf22 "w"});\par
00117     {\cf19 if} (archivo == NULL)\par
00118     \{\par
00119         printf({\cf22 "Error al abrir el archivo %s.\\n"}, AdminProv_txt);\par
00120         {\cf19 return};\par
00121     \}\par
00122 \par
00123     {\cf19 for} ({\cf18 int} i = 0; i < numAdmins; i++)\par
00124     \{\par
00125         fprintf(archivo, {\cf22 "%s-%s-%s-%s-%s-\\n"},\par
00126                 admins[i].id_empresa,\par
00127                 admins[i].nombre,\par
00128                 admins[i].email,\par
00129                 admins[i].contrasenia,\par
00130                 admins[i].perfil_usuario);\par
00131     \}\par
00132 \par
00133     fclose(archivo);\par
00134 \}\par
00135 {\cf20 // Funci\'F3n para iniciar el vector de administradores o proveedores desde un archivo}\par
00136 AdminProv *iniciarAdminProvDeArchivo({\cf18 int} *numAdmins)\par
00137 \{\par
00138     FILE *archivo = fopen(AdminProv_txt, {\cf22 "r"});\par
00139     {\cf19 if} (archivo == NULL)\par
00140     \{\par
00141         printf({\cf22 "Error al abrir el archivo AdminProv.txt.\\n"});\par
00142         {\cf19 return} NULL;\par
00143     \}\par
00144 \par
00145     {\cf20 // Contar la cantidad de l\'EDneas en el archivo}\par
00146     {\cf18 int} count = 0;\par
00147     {\cf18 char} buffer[TAMANIO_MAXIMO_LINEA]; {\cf20 // Longitud m\'E1xima de l\'EDnea}\par
00148     {\cf19 while} (fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\par
00149     \{\par
00150         count++;\par
00151     \}\par
00152 \par
00153     {\cf20 // Regresar al inicio del archivo}\par
00154     rewind(archivo);\par
00155 \par
00156     {\cf20 // Crear el vector de administradores o proveedores}\par
00157     AdminProv *adminProvs = (AdminProv *) malloc(count * {\cf17 sizeof}(AdminProv));\par
00158     {\cf19 if} (adminProvs == NULL)\par
00159     \{\par
00160         fclose(archivo);\par
00161         printf({\cf22 "Error: No se pudo asignar memoria para el vector de AdminProv.\\n"});\par
00162         {\cf19 return} NULL;\par
00163     \}\par
00164 \par
00165     {\cf20 // Leer cada l\'EDnea y rellenar el vector de administradores o proveedores}\par
00166     {\cf19 for}({\cf18 int} i=0; i < count; i++)\par
00167     \{\par
00168         {\cf19 if}(fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\par
00169         \{\par
00170             {\cf18 char} *token = strtok(buffer, {\cf22 "-"});\par
00171             strncpy(adminProvs[i].id_empresa, token, {\cf17 sizeof}(adminProvs[i].id_empresa));\par
00172             token = strtok(NULL, {\cf22 "-"});\par
00173             strncpy(adminProvs[i].nombre, token, {\cf17 sizeof}(adminProvs[i].nombre));\par
00174             token = strtok(NULL, {\cf22 "-"});\par
00175             strncpy(adminProvs[i].email, token, {\cf17 sizeof}(adminProvs[i].email));\par
00176             token = strtok(NULL, {\cf22 "-"});\par
00177             strncpy(adminProvs[i].contrasenia, token, {\cf17 sizeof}(adminProvs[i].contrasenia));\par
00178             token = strtok(NULL, {\cf22 "-"});\par
00179             strncpy(adminProvs[i].perfil_usuario, token, {\cf17 sizeof}(adminProvs[i].perfil_usuario));\par
00180         \}\par
00181     \}\par
00182 \par
00183     fclose(archivo);\par
00184     *numAdmins = count;\par
00185     {\cf19 return} adminProvs;\par
00186 \}\par
00187 \par
00188 {\cf20 /*}\par
00189 {\cf20 int main() \{}\par
00190 {\cf20     AdminProv *administradores_proveedores = NULL;}\par
00191 {\cf20     int tamano_admin_prov = 0;}\par
00192 {\cf20     int opcion;}\par
00193 {\cf20     char id_baja[5];}\par
00194 {\cf20 }\par
00195 {\cf20     do \{}\par
00196 {\cf20         printf("\\n1. Alta de administrador/proveedor");}\par
00197 {\cf20         printf("\\n2. Baja de administrador/proveedor");}\par
00198 {\cf20         printf("\\n3. Listado de administradores/proveedores");}\par
00199 {\cf20         printf("\\n4. Salir");}\par
00200 {\cf20         printf("\\nSeleccione una opcion: ");}\par
00201 {\cf20         scanf("%d", &opcion);}\par
00202 {\cf20 }\par
00203 {\cf20         switch(opcion) \{}\par
00204 {\cf20             case 1:}\par
00205 {\cf20                 alta_AdminProv(&administradores_proveedores, &tamano_admin_prov);}\par
00206 {\cf20                 break;}\par
00207 {\cf20             case 2:}\par
00208 {\cf20                 printf("Ingrese el ID del administrador/proveedor a dar de baja: ");}\par
00209 {\cf20                 scanf("%s", id_baja);}\par
00210 {\cf20                 baja_adminprov(administradores_proveedores, &tamano_admin_prov, id_baja);}\par
00211 {\cf20                 break;}\par
00212 {\cf20             case 3:}\par
00213 {\cf20                 listado_adminprov(administradores_proveedores, tamano_admin_prov);}\par
00214 {\cf20                 break;}\par
00215 {\cf20             case 4:}\par
00216 {\cf20                 printf("Saliendo...\\n");}\par
00217 {\cf20                 break;}\par
00218 {\cf20             default:}\par
00219 {\cf20                 printf("Opci\'F3n inv\'E1lida. Por favor, seleccione una opci\'F3n v\'E1lida.\\n");}\par
00220 {\cf20         \}}\par
00221 {\cf20     \} while(opcion != 4);}\par
00222 {\cf20 }\par
00223 {\cf20     // Liberar la memoria asignada para el arreglo de administradores/proveedores}\par
00224 {\cf20     free(administradores_proveedores);}\par
00225 {\cf20 }\par
00226 {\cf20     return 0;}\par
00227 {\cf20 \}*/}\par
}
