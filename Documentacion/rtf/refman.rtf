00258 \par
00259     {\cf20 // Leer cada linea y rellenar el vector}\par
00260     {\cf19 for}({\cf18 int} i=0;i<count;i++)\par
00261     \{\par
00262         {\cf19 if}(fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\{\par
00263         {\cf18 char} *token = strtok(buffer, {\cf22 "-"});\par
