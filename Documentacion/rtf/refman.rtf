00029 {\cf18 void} alta_locker(Locker **l, {\cf18 int}* tamanio_vector)\{\par
00030 \par
00031     Locker nuevo_locker;\par
00032     {\cf20 // Genera un nuevo ID para el locker}\par
00033     {\cf18 char} *cadena = id_generator_locker(*l, *tamanio_vector);\par
00034     strcpy(nuevo_locker.id_locker, cadena);\par
00035     free(cadena); {\cf20 // Libera la memoria asignada para el ID generado}\par
00036 \par
00037     {\cf20 // Solicita al usuario informaci\'F3n sobre el nuevo locker}\par
00038     printf({\cf22 "\\nDime la localidad"});\par
00039     scanf({\cf22 "%20s"}, nuevo_locker.localidad);\par
00040     flushInputBuffer();\par
00041 \par
00042     printf({\cf22 "\\nDime la provincia"});\par
00043     scanf({\cf22 "%20s"}, nuevo_locker.provincia);\par
00044     flushInputBuffer();\par
00045 \par
00046     printf({\cf22 "\\nDime la direccion"});\par
00047     scanf({\cf22 "%20s"}, nuevo_locker.ubicacion);\par
00048     flushInputBuffer();\par
00049 \par
00050     printf({\cf22 "\\nDime cuantos lockers hay en total"});\par
00051     scanf({\cf22 "%d"}, &nuevo_locker.num_compt);\par
00052     {\cf20 // Realiza una realocaci\'F3n de memoria para agregar el nuevo locker al arreglo}\par
00053     *l = (Locker *)realloc(*l, (*tamanio_vector + 1) * {\cf17 sizeof}(Locker));\par
00054     (*l)[*tamanio_vector] = nuevo_locker; {\cf20 // Guarda el nuevo locker en el arreglo}\par
00055     (*tamanio_vector)++;\par
00056     nuevo_locker.num_compocup=0;\par
