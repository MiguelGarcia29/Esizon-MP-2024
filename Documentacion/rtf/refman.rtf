00031 {\cf20 // Funci\'F3n para dar de alta un nuevo descuento}\par
00032 {\cf18 void} alta_descuentos(Descuento **desc, {\cf18 int} *cantdad_desc) \{\par
00033     {\cf18 int} a;\par
00034     Descuento nuevo_descuento;\par
00035 \par
00036     printf({\cf22 "Descripcion: "});\par
00037     fgets(nuevo_descuento.descrip, 51, stdin);\par
00038     nuevo_descuento.descrip[strcspn(nuevo_descuento.descrip, {\cf22 "\\n"})] = {\cf23 '\\0'};\par
00039 \par
00040     printf({\cf22 "Tipo:\\n"});\par
00041     printf({\cf22 "(1) Codigo promocional\\n"});\par
00042     printf({\cf22 "(2) Cheque regalo\\n"});\par
00043     scanf({\cf22 "%d"}, &a);\par
00044     flushInputBuffer();\par
00045 \par
00046     {\cf19 switch} (a) \{\par
00047         {\cf19 case} 1:\par
00048             strcpy(nuevo_descuento.tipo, {\cf22 "codpro"});\par
00049         {\cf19 break};\par
00050         {\cf19 case} 2:\par
00051             strcpy(nuevo_descuento.tipo, {\cf22 "cheqreg"});\par
00052         {\cf19 break};\par
00053         {\cf19 default}:\par
00054             printf({\cf22 "\\nError\\n\\n"});\par
00055             exit(EXIT_FAILURE);\par
00056         {\cf19 break};\par
00057     \}\par
00058 \par
00059     printf({\cf22 "Aplicabilidad:\\n"});\par
00060     printf({\cf22 "(1) Todos\\n"});\par
00061     printf({\cf22 "(2) Solo ESIZON\\n"});\par
00062     scanf({\cf22 "%d"}, &a);\par
00063     flushInputBuffer();\par
00064 \par
00065     {\cf19 switch} (a) \{\par
00066         {\cf19 case} 1:\par
00067             strcpy(nuevo_descuento.aplicable, {\cf22 "todos"});\par
00068         {\cf19 break};\par
00069         {\cf19 case} 2:\par
00070             strcpy(nuevo_descuento.aplicable, {\cf22 "esizon"});\par
00071         {\cf19 break};\par
00072         {\cf19 default}:\par
00073             printf({\cf22 "\\nError\\n\\n"});\par
00074             exit(EXIT_FAILURE);\par
00075         {\cf19 break};\par
00076     \}\par
00077 \par
00078     printf({\cf22 "Importe: "});\par
00079     scanf({\cf22 "%f"}, &nuevo_descuento.importe);\par
00080     flushInputBuffer();\par
00081 \par
00082     printf({\cf22 "Estado:\\n"});\par
00083     printf({\cf22 "(1) Activo\\n"});\par
00084     printf({\cf22 "(2) Inactivo\\n"});\par
