00153 \par
00154     {\cf20 // Contar la cantidad de lineas en el archivo}\par
00155     {\cf18 int} count = 0;\par
00156     {\cf18 char} buffer[TAMANIO_MAXIMO_LINEA]; {\cf20 // Longitud maxima de linea}\par
00157     {\cf19 while} (fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\par
00158     \{\par
00159         count++;\par
00160     \}\par
00161 \par
00162     {\cf20 // Regresar al inicio del archivo}\par
00163     rewind(archivo);\par
00164 \par
00165     {\cf20 // Crear el vector de Locker}\par
00166     Locker *lockers = (Locker *)malloc(count * {\cf17 sizeof}(Locker));\par
00167     {\cf19 if} (lockers == NULL)\par
00168     \{\par
00169         fclose(archivo);\par
00170         printf({\cf22 "Error: No se pudo asignar memoria para el vector de Locker.\\n"});\par
00171         {\cf19 return} NULL;\par
00172     \}\par
00173 \par
00174     {\cf20 // Leo cada linea y rellenar el vector de adminProv}\par
00175     {\cf19 for}({\cf18 int} i=0;i<count;i++)\par
00176     \{\par
00177         {\cf19 if}(fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\{\par
00178         {\cf18 char} *token = strtok(buffer, {\cf22 "-"});\par
00179         strncpy(lockers[i].id_locker, token, {\cf17 sizeof}(lockers[i].id_locker));\par
00180         token = strtok(NULL, {\cf22 "-"});\par
