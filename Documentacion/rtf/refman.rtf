00147 {\cf18 void} bajaProveedor(AdminProv **proveedores, {\cf18 int} *numProveedores,{\cf18 char} *{\cf18 id}) \{\par
00148 \par
00149     {\cf18 int} encontrado = 0;\par
00150 \par
00151     {\cf19 for} ({\cf18 int} i = 0; i < *numProveedores && encontrado == 0; i++) \{\par
00152         {\cf19 if} (strcmp((*proveedores)[i].id_empresa, {\cf18 id}) == 0) \{\par
00153             encontrado = 1;\par
00154             {\cf20 // Eliminar el proveedor moviendo los elementos hacia adelante}\par
00155             {\cf19 for} ({\cf18 int} j = i; j < *numProveedores - 1; j++) \{\par
