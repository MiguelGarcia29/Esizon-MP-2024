00043 {\cf20 // FUNCION QUE DA INICIO AL SISTEMA}\par
00044 {\cf18 void} accederPrograma(Cliente **clientes, {\cf18 int} *numClientes, AdminProv **adminProvs, {\cf18 int} *numAdminProvs,\par
00045                       Categoria **categorias, {\cf18 int} *numCategorias, CompartimentoLocker **compartimentoLockers, {\cf18 int} *numCompartimentoLockers,\par
00046                       DescuentoCliente **descuentoClientes, {\cf18 int} *numDescuentoClientes, Descuento **descuentos, {\cf18 int} *numDescuentos,\par
00047                       Devolucion **devoluciones, {\cf18 int} *numDevoluciones, Locker **lockers, {\cf18 int} *numLockers, Pedido **pedidos, {\cf18 int} *numPedido,\par
00048                       Producto **productos, {\cf18 int} *numProductos, ProductoPedido **productoPedidos, {\cf18 int} *numProductoPedidos,\par
00049                       Transportista **transportistas, {\cf18 int} *numTransportistas)\par
00050 \{\par
00051     {\cf20 // Declaraci\'F3n de variables locales}\par
00052     {\cf18 int} rol = 6; {\cf20 // Inicializa el rol como 6 (ning\'FAn rol definido)}\par
00053     {\cf18 int} posVectorClienteActual; {\cf20 // Variable para almacenar la posici\'F3n del cliente actual}\par
00054 \par
00055     {\cf20 // Impresi\'F3n del mensaje de bienvenida}\par
00056     {\cf20 // Se imprime un dise\'F1o gr\'E1fico y se solicitan las credenciales de inicio de sesi\'F3n}\par
00057 \par
00058     printf({\cf22 "\\n"});\par
00059     printf({\cf22 "   ########  #######   ######     ##########  ##########  ###      ##\\n"});\par
00060     printf({\cf22 "   ##       ##           ##              #    ##      ##  ## #     ##\\n"});\par
00061     printf({\cf22 "   ##       ##           ##             #     ##      ##  ##  #    ##\\n"});\par
00062     printf({\cf22 "   #####     ######      ##         #####     ##      ##  ##   #   ##\\n"});\par
00063     printf({\cf22 "   ##              ##    ##         #         ##      ##  ##    #  ##\\n"});\par
00064     printf({\cf22 "   ##              ##    ##        #          ##      ##  ##     # ##\\n"});\par
00065     printf({\cf22 "   #######    #####    ######     ########    ##########  ##      ###\\n"});\par
00066     HANDLE hConsole = GetStdHandle(STD_OUTPUT_HANDLE);{\cf20 // Se encarga de mostrar el color de los caracteres}\par
00067     SetConsoleTextAttribute(hConsole, FOREGROUND_RED | FOREGROUND_GREEN | FOREGROUND_INTENSITY); {\cf20 // Se encarga de mostrar el color de los caracteres}\par
00068     printf({\cf22 "                                                        ##\\n"});\par
00069     printf({\cf22 "          ##                                          ##  ##\\n"});\par
00070     printf({\cf22 "           ##                                       ##     ##\\n"});\par
00071     printf({\cf22 "            ##                                   ##   ##    ##\\n"});\par
00072     printf({\cf22 "              ###                                   ###       ##\\n"});\par
00073     printf({\cf22 "                 ###                             ###\\n"});\par
00074     printf({\cf22 "                    ####                     ####  \\n"});\par
00075     printf({\cf22 "                        #######       #######\\n"});\par
00076     printf({\cf22 "                               #######\\n"});\par
00077     SetConsoleTextAttribute(hConsole, FOREGROUND_RED | FOREGROUND_GREEN | FOREGROUND_BLUE);{\cf20 // Se encarga de mostrar el color de los caracteres}\par
00078 \par
00079     {\cf20 // COMPROBAR QUE EL ROL EST\'C9 REGISTRADO}\par
00080     {\cf20 // Ciclo para el inicio de sesi\'F3n}\par
00081     {\cf19 do}\par
00082     \{\par
00083         printf({\cf22 "Correo electronico: "});\par
00084         {\cf18 char} correo[31];\par
00085         fgets(correo, 31, stdin);\par
00086         {\cf20 // Elimina el \\n}\par
00087         correo[strcspn(correo, {\cf22 "\\n"})] = 0;\par
00088 \par
00089         printf({\cf22 "Clave de acceso: "});\par
00090         {\cf18 char} contrasenia[16];\par
00091         fgets(contrasenia, 16, stdin);\par
00092         contrasenia[strcspn(contrasenia, {\cf22 "\\n"})] = 0;\par
00093 \par
00094         {\cf20 // Llama a la funci\'F3n iniciarSesion para validar las credenciales y obtener el rol del usuario}\par
00095         rol = iniciarSesion(&posVectorClienteActual, *clientes, *numClientes, *adminProvs, *numAdminProvs, *transportistas, *numTransportistas, correo, contrasenia);\par
00096 \par
00097     \} {\cf19 while} (rol == 6); {\cf20 // Repetir hasta que se inicie sesi\'F3n con \'E9xito}\par
00098 \par
00099     {\cf20 // Una vez iniciada la sesi\'F3n, se redirige al usuario seg\'FAn su rol}\par
00100     {\cf19 switch} (rol)\par
00101     \{\par
00102     {\cf19 case} 1: {\cf20 // Administrador}\par
00103         menuadmin(rol, posVectorClienteActual, clientes, numClientes, adminProvs, numAdminProvs, transportistas, numTransportistas, productos, numProductos, categorias, numCategorias, numProductoPedidos);\par
00104         {\cf19 break};\par
00105     {\cf19 case} 2: {\cf20 // Usuario (cliente)}\par
00106         menuusuario(rol, posVectorClienteActual, clientes, numClientes, adminProvs, numAdminProvs, transportistas, numTransportistas, productos, numProductos, categorias, numCategorias, numProductoPedidos);\par
00107         {\cf19 break};\par
00108     {\cf19 case} 3: {\cf20 // Proveedor}\par
00109         menuproveedor(rol, posVectorClienteActual, clientes, numClientes, adminProvs, numAdminProvs, transportistas, numTransportistas, productos, numProductos, categorias, numCategorias, productoPedidos, numProductoPedidos);\par
00110         {\cf19 break};\par
00111     {\cf19 case} 4: {\cf20 // Transportista}\par
00112         menutransportista(rol, posVectorClienteActual, lockers, numLockers, clientes, numClientes, adminProvs, numAdminProvs, transportistas, numTransportistas, productos, numProductos, categorias, numCategorias, numProductoPedidos, productoPedidos, compartimentoLockers, numCompartimentoLockers);\par
00113         {\cf19 break};\par
00114     \}\par
00115 \}\par
00116 \par
00117 {\cf20 // Funci\'F3n para iniciar sesi\'F3n}\par
00118 {\cf20 // Devuelve el rol del usuario que inici\'F3 sesi\'F3n}\par
00119 {\cf20 // Devuelve 0 si pudo iniciar sesi\'F3n en el rol indicado y 1 si no}\par
00120 {\cf18 int} iniciarSesion({\cf18 int} *posVectorActual, Cliente *clientes, {\cf18 int} numClientes, AdminProv *adminProvs, {\cf18 int} numAdminProvs, Transportista *transportistas, {\cf18 int} numTransportistas, {\cf18 char} *correo, {\cf18 char} *contrasenia)\par
00121 \{\par
00122     {\cf20 // Variable para almacenar el rol del usuario}\par
00123     {\cf18 int} rol = 6;\par
00124 \par
00125     {\cf20 // Comprobar si el usuario es un administrador}\par
00126     {\cf19 if} (comprobarAdminoProv(correo, contrasenia, {\cf22 "administrador"}, adminProvs, numAdminProvs, posVectorActual) == 0)\par
00127         rol = 1; {\cf20 // Rol de administrador}\par
00128 \par
00129     {\cf20 // Comprobar si el usuario es un cliente}\par
00130     {\cf19 if} (comprobarUsuario(correo, contrasenia, clientes, numClientes, posVectorActual) == 0)\par
00131         rol = 2; {\cf20 // Rol de cliente}\par
00132 \par
00133     {\cf20 // Comprobar si el usuario es un proveedor}\par
00134     {\cf19 if} (comprobarAdminoProv(correo, contrasenia, {\cf22 "proveedor"}, adminProvs, numAdminProvs, posVectorActual) == 0)\par
00135         rol = 3; {\cf20 // Rol de proveedor}\par
00136 \par
00137     {\cf20 // Comprobar si el usuario es un transportista}\par
00138     {\cf19 if} (comprobarTransportista(correo, contrasenia, transportistas, numTransportistas, posVectorActual) == 0)\par
00139         rol = 4; {\cf20 // Rol de transportista}\par
00140 \par
00141     {\cf20 // Mensaje de error si no se pudo iniciar sesi\'F3n}\par
00142     {\cf19 if} (rol == 6)\par
00143     \{\par
00144         printf({\cf22 "Alg\'FAn dato es incorrecto\\n"});\par
00145     \}\par
00146 \par
00147     {\cf20 // Devolver el rol del usuario}\par
00148     {\cf19 return} rol;\par
00149 \}\par
00150 \par
00151 {\cf20 // Funci\'F3n para comprobar las credenciales de un administrador o proveedor}\par
00152 {\cf18 int} comprobarAdminoProv({\cf18 char} *email, {\cf18 char} *contrasenia, {\cf18 char} *rol, AdminProv *adminProvs, {\cf18 int} numAdminProvs, {\cf18 int} *posVectorClienteActual)\par
00153 \{\par
00154     {\cf20 // Variable para indicar si se ha encontrado una coincidencia de credenciales}\par
00155     {\cf18 int} encontrado = 1;\par
00156 \par
00157     {\cf20 // Recorre el arreglo de administradores y proveedores}\par
00158     {\cf19 for} ({\cf18 int} i = 0; i < numAdminProvs && encontrado == 1; i++)\par
00159     \{\par
00160         {\cf20 // Verifica si el correo electr\'F3nico, contrase\'F1a y rol coinciden}\par
00161         {\cf19 if} (strcmp(adminProvs[i].email, email) == 0 && strcmp(adminProvs[i].contrasenia, contrasenia) == 0 && strcmp(adminProvs[i].perfil_usuario, rol) == 0)\par
00162         \{\par
00163             {\cf20 // Si hay coincidencia, actualiza las variables y sale del bucle}\par
00164             encontrado = 0;\par
00165             *posVectorClienteActual = i; {\cf20 // Se actualiza la posici\'F3n del vector del cliente}\par
00166         \}\par
00167     \}\par
00168 \par
00169     {\cf20 // Devuelve 1 si no se encuentra coincidencia, 0 si se encuentra}\par
00170     {\cf19 return} encontrado;\par
00171 \}\par
00172 \par
00173 \par
00174 {\cf20 // Funci\'F3n para comprobar las credenciales de un usuario}\par
00175 {\cf18 int} comprobarUsuario({\cf18 char} *email, {\cf18 char} *contrasenia, Cliente *clientes, {\cf18 int} numClientes, {\cf18 int} *posVectorClienteActual)\par
00176 \{\par
00177     {\cf20 // Variable para indicar si se ha encontrado una coincidencia de credenciales}\par
00178     {\cf18 int} encontrado = 1;\par
00179 \par
00180     {\cf20 // Recorre el arreglo de clientes}\par
00181     {\cf19 for} ({\cf18 int} i = 0; i < numClientes && encontrado == 1; i++)\par
00182     \{\par
00183         {\cf20 // Verifica si el correo electr\'F3nico y la contrase\'F1a coinciden}\par
00184         {\cf19 if} (strcmp(clientes[i].email, email) == 0 && strcmp(clientes[i].contrasenia, contrasenia) == 0)\par
00185         \{\par
