00003 {\cf20 // Funci\'F3n para generar un ID \'FAnico para un nuevo locker}\par
00004 {\cf18 char}* id_generator_locker(Locker *l, {\cf18 int} tamanio_vector) \{\par
00005     {\cf18 int} id_generada = 1;\par
00006 \par
00007     {\cf20 // Si hay lockers en el vector, obtiene el \'FAltimo ID y genera uno nuevo increment\'E1ndolo}\par
00008     {\cf19 if} (tamanio_vector != 0) \{\par
00009         id_generada = atoi(l[tamanio_vector - 1].id_locker + 4) + 1;\par
00010     \}\par
00011 \par
00012     {\cf20 // Asigna memoria para el ID generado}\par
00013     {\cf18 char}* {\cf18 id} = malloc(11 * {\cf17 sizeof}({\cf18 char}));\par
00014     {\cf19 if} ({\cf18 id} == NULL) \{\par
00015         printf({\cf22 "Error al asignar memoria. \\n"});\par
00016         exit(EXIT_FAILURE);\par
00017     \}\par
00018 \par
00019     {\cf20 // Formatea la nueva ID y la guarda en el arreglo}\par
00020     {\cf18 char} id2[5];\par
00021     sprintf(id2, {\cf22 "%03d"}, id_generada);\par
00022     strcpy({\cf18 id}, {\cf22 "Lock"});\par
00023     strcat({\cf18 id}, id2);\par
00024 \par
00025     {\cf19 return} id;\par
00026 \}\par
