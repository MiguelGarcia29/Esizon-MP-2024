00005 {\cf20 // Depura el buffer.}\par
00006 {\cf18 void} flushInputBufferr()\par
00007 \{\par
00008     {\cf18 int} c;\par
00009     {\cf19 while} ((c = getchar()) != {\cf23 '\\n'} && c != EOF)\par
00010         ;\par
00011 \}\par
00012 \par
00013 \par
00014 {\cf20 // Guarda el vector de Produtcos en el archivo siguiendo la estructura:}\par
00015 {\cf20 /*}\par
00016 {\cf20     o Identificador del producto (Id_prod), 7 digitos.}\par
00017 {\cf20     o Descripci\'F3n del producto (Descrip), 50 caracteres maximo.}\par
00018 {\cf20     o Id de la categoria a la que pertenece (Id_categ), 4 digitos.}\par
00019 {\cf20     o Id del gestor del producto (Id_gestor), 4 digitos. Debera coincidir con el identificador de la}\par
00020 {\cf20     empresa administradora o proveedora del producto.}\par
00021 {\cf20     o Stock del producto (Stock).}\par
00022 {\cf20     o Compromiso de Entrega (Entrega). Indicara el n\'FAmero de dias maximo que transcurrira desde}\par
00023 {\cf20     la fecha de realizaci\'F3n del pedido hasta la entrega al cliente.}\par
00024 {\cf20     o Importe del producto en euros (Importe).}\par
00025 {\cf20 */}\par
00026 {\cf18 void} guardarProductosEnArchivo(Producto *productos, {\cf18 int} numProductos)\par
00027 \{\par
00028     FILE *archivo = fopen(Productos_txt, {\cf22 "w"});\par
00029     {\cf19 if} (archivo == NULL)\par
00030     \{\par
00031         printf({\cf22 "Error al abrir el archivo Productos.txt.\\n"});\par
00032         {\cf19 return};\par
00033     \}\par
00034 \par
00035     {\cf19 for} ({\cf18 int} i = 0; i < numProductos; i++)\par
00036     \{\par
00037         fprintf(archivo, {\cf22 "%s-%s-%s-%s-%s-%d-%d-%.2f\\n"},\par
00038                 productos[i].id_prod,\par
00039                 productos[i].nombre,\par
00040                 productos[i].descrip,\par
00041                 productos[i].id_categ,\par
00042                 productos[i].id_gestor,\par
00043                 productos[i].stock,\par
00044                 productos[i].entrega,\par
00045                 productos[i].importe);\par
