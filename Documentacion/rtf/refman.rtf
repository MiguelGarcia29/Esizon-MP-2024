00430 {\cf20     o Ciudad de reparto (Ciudad), 20 caracteres m\'E1ximo.}\par
00431 {\cf20 */}\par
00432 {\cf18 void} guardarTransportistasEnArchivo(Transportista *transportistas, {\cf18 int} numTransportistas)\par
00433 \{\par
00434     FILE *archivo = fopen(Transportistas_txt, {\cf22 "w"});\par
00435     {\cf19 if} (archivo == NULL)\par
00436     \{\par
00437         printf({\cf22 "Error al abrir el archivo Transportistas.txt.\\n"});\par
00438         {\cf19 return};\par
00439     \}\par
00440 \par
00441     {\cf19 for} ({\cf18 int} i = 0; i < numTransportistas; i++)\par
00442     \{\par
00443         fprintf(archivo, {\cf22 "%s-%s-%s-%s-%s-%s-\\n"},\par
00444                 transportistas[i].id_transp,\par
00445                 transportistas[i].nombre,\par
00446                 transportistas[i].email,\par
00447                 transportistas[i].contrasenia,\par
00448                 transportistas[i].nombre_empresa,\par
00449                 transportistas[i].ciudad_reparto);\par
00450     \}\par
00451 \par
00452     fclose(archivo);\par
00453 \}\par
00454 \par
00455 Transportista *iniciarTransportistasDeArchivo({\cf18 int} *numTransportista)\par
00456 \{\par
00457     FILE *archivo = fopen(Transportistas_txt, {\cf22 "r"});\par
00458     {\cf19 if} (archivo == NULL)\par
00459     \{\par
00460         printf({\cf22 "Error al abrir el archivo %s.\\n"}, Transportistas_txt);\par
00461         {\cf19 return} NULL;\par
00462     \}\par
00463 \par
00464     {\cf20 // Contar la cantidad de lineas en el archivo}\par
00465     {\cf18 int} count = 0;\par
00466     {\cf18 char} buffer[TAMANIO_MAXIMO_LINEA]; {\cf20 // Longitud maxima de linea}\par
00467     {\cf19 while} (fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\par
00468     \{\par
00469         count++;\par
00470     \}\par
00471 \par
00472     {\cf20 // Regresar al inicio del archivo}\par
00473     rewind(archivo);\par
00474 \par
00475     {\cf20 // Crear el vector de Locker}\par
00476     Transportista *transportistas = (Transportista *)malloc(count * {\cf17 sizeof}(Transportista));\par
00477     {\cf19 if} (transportistas == NULL)\par
00478     \{\par
00479         fclose(archivo);\par
00480         printf({\cf22 "Error: No se pudo asignar memoria para el vector de Transportistas.\\n"});\par
00481         {\cf19 return} NULL;\par
00482     \}\par
00483 \par
00484     {\cf20 // Leo cada linea y rellenar el vector de adminProv}\par
00485     {\cf19 for}({\cf18 int} i=0;i<count;i++)\par
00486     \{\par
00487         {\cf19 if}(fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\{\par
00488         {\cf18 char} *token = strtok(buffer, {\cf22 "-"});\par
00489         strncpy(transportistas[i].id_transp, token, {\cf17 sizeof}(transportistas[i].id_transp));\par
00490         token = strtok(NULL, {\cf22 "-"});\par
00491         strncpy(transportistas[i].nombre, token, {\cf17 sizeof}(transportistas[i].nombre));\par
00492         token = strtok(NULL, {\cf22 "-"});\par
00493         strncpy(transportistas[i].email, token, {\cf17 sizeof}(transportistas[i].email));\par
00494         token = strtok(NULL, {\cf22 "-"});\par
00495         strncpy(transportistas[i].contrasenia, token, {\cf17 sizeof}(transportistas[i].contrasenia));\par
00496         token = strtok(NULL, {\cf22 "-"});\par
00497         strncpy(transportistas[i].nombre_empresa, token, {\cf17 sizeof}(transportistas[i].nombre_empresa));\par
00498         token = strtok(NULL, {\cf22 "-"});\par
00499         strncpy(transportistas[i].ciudad_reparto, token, {\cf17 sizeof}(transportistas[i].ciudad_reparto));\par
00500 \par
00501         \}\par
00502     \}\par
00503     fclose(archivo);\par
00504     *numTransportista = count;\par
00505     {\cf19 return} transportistas;\par
00506 \}\par
