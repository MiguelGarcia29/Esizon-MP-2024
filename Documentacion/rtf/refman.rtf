00173 {\cf18 void} buscarProveedor(AdminProv **proveedores, {\cf18 int} *numProveedores) \{\par
00174     {\cf18 char} {\cf18 id}[5];\par
00175     {\cf18 int} encontrado = 0;\par
00176     printf({\cf22 "Ingrese el ID: "});\par
00177     fgets({\cf18 id}, 5, stdin);\par
00178     {\cf18 id}[strcspn({\cf18 id}, {\cf22 "\\n"})] = {\cf23 '\\0'};\par
