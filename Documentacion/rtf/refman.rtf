00138     {\cf19 for} ({\cf18 int} i = 0; i < *tamanio; i++) \{\par
00139         {\cf19 if} (strcmp((*pedidos)[i].id_cliente, idC) == 0) \{\par
00140             printf({\cf22 "ID Pedido: %s\\n"}, (*pedidos)[i].id_pedido);\par
