00077 {\cf20 // Funci\'F3n para dar de baja un locker existente}\par
00078 {\cf18 void} baja_locker(Locker *l , {\cf18 int}* tamanio, {\cf18 char} *id_baja)\{\par
00079 \par
00080     {\cf18 int} encontrado = 0;\par
00081 \par
00082     {\cf20 // Busca el locker con el ID dado y lo elimina del arreglo}\par
00083     {\cf19 for}({\cf18 int} i = 0 ; i < *tamanio ; i++) \{\par
00084         {\cf19 if}(strcmp(l[i].id_locker, id_baja) == 0)\{\par
00085             encontrado = 1;\par
00086 \par
00087             {\cf20 // Reorganiza el arreglo para eliminar el locker encontrado}\par
00088             {\cf19 for}({\cf18 int} j = i; j < *tamanio - 1; j++) \{\par
00089                 l[j] = l[j + 1];\par
00090             \}\par
00091 \par
00092             (*tamanio)--; {\cf20 // Reduce el tama\'F1o del arreglo}\par
00093             printf({\cf22 "Locker con ID %s ha sido dado de baja correctamente.\\n"}, id_baja);\par
00094             {\cf19 break}; {\cf20 // Sale del bucle una vez encontrado y eliminado el locker}\par
00095         \}\par
00096     \}\par
00097 \par
00098     {\cf19 if}(encontrado == 0)\{\par
00099         printf({\cf22 "La ID registrada no ha sido encontrada.\\n"});\par
00100     \}\par
