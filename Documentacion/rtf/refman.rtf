00276 \par
00277 {\cf20 // Guarda el vector de DescuentosClientes en el archivo siguiendo la estructura:}\par
00278 {\cf20 /*}\par
00279 {\cf20     o Identificador del cliente (Id_cliente), 7 d\'EDgitos.}\par
00280 {\cf20     o Identificador del c\'F3digo promocional o cheque regalo (Id_cod), 10 caracteres m\'E1ximo.}\par
00281 {\cf20     o Fecha de asignaci\'F3n (d\'EDa, mes y a\'F1o)}\par
00282 {\cf20     o Fecha de caducidad (d\'EDa, mes y a\'F1o)}\par
00283 {\cf20     o Estado (Estado): 1 (Aplicado) \'F3 0 (No aplicado)}\par
00284 {\cf20 */}\par
00285 {\cf18 void} guardarDescuentosClientesEnArchivo(DescuentoCliente *descuentosClientes, {\cf18 int} numDescuentosClientes)\par
00286 \{\par
00287     FILE *archivo = fopen(DescuentoCliente_txt, {\cf22 "w"});\par
00288     {\cf19 if} (archivo == NULL)\par
00289     \{\par
00290         printf({\cf22 "Error al abrir el archivo DescuentosClientes.txt.\\n"});\par
00291         {\cf19 return};\par
00292     \}\par
00293 \par
00294     {\cf19 for} ({\cf18 int} i = 0; i < numDescuentosClientes; i++)\par
00295     \{\par
00296         fprintf(archivo, {\cf22 "%s-%s-%s-%s-%d\\n"},\par
00297                 descuentosClientes[i].id_cliente,\par
00298                 descuentosClientes[i].id_cod,\par
00299                 descuentosClientes[i].fecha_asignacion,\par
00300                 descuentosClientes[i].fecha_caducidad,\par
00301                 descuentosClientes[i].estado);\par
00302     \}\par
00303 \par
00304     fclose(archivo);\par
00305 \}\par
00306 \par
00307 DescuentoCliente *iniciarDescuentosClientesDeArchivo({\cf18 int} *numDescC)\par
00308 \{\par
00309     FILE *archivo = fopen(DescuentoCliente_txt, {\cf22 "r"});\par
00310     {\cf19 if} (archivo == NULL)\par
00311     \{\par
00312         printf({\cf22 "Error al abrir el archivo %s.\\n"}, DescuentoCliente_txt);\par
00313         {\cf19 return} NULL;\par
00314     \}\par
00315 \par
00316     {\cf20 // Contar la cantidad de lineas en el archivo}\par
00317     {\cf18 int} count = 0;\par
00318     {\cf18 char} buffer[TAMANIO_MAXIMO_LINEA]; {\cf20 // Longitud maxima de linea}\par
00319     {\cf19 while} (fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\par
00320     \{\par
00321         count++;\par
00322     \}\par
00323 \par
00324     {\cf20 // Regresar al inicio del archivo}\par
00325     rewind(archivo);\par
00326 \par
00327     {\cf20 // Crear el vector de DescuentoCliente}\par
00328     DescuentoCliente *descuentos =(DescuentoCliente *) malloc(count * {\cf17 sizeof}(DescuentoCliente));\par
00329     {\cf19 if} (descuentos == NULL)\par
00330     \{\par
00331         fclose(archivo);\par
00332         printf({\cf22 "Error: No se pudo asignar memoria para el vector de DescuentoCliente.\\n"});\par
00333         {\cf19 return} NULL;\par
00334     \}\par
00335 \par
00336     {\cf20 // Leo cada linea y rellenar el vector de adminProv}\par
00337     {\cf19 for}({\cf18 int} i=0;i<count;i++)\par
00338     \{\par
00339         {\cf19 if}(fgets(buffer, TAMANIO_MAXIMO_LINEA, archivo) != NULL)\{\par
00340         {\cf18 char} *token = strtok(buffer, {\cf22 "-"});\par
00341         strncpy(descuentos[i].id_cliente, token, {\cf17 sizeof}(descuentos[i].id_cliente));\par
00342         token = strtok(NULL, {\cf22 "-"});\par
00343         strncpy(descuentos[i].id_cod, token, {\cf17 sizeof}(descuentos[i].id_cod));\par
00344         token = strtok(NULL, {\cf22 "-"});\par
00345         strncpy(descuentos[i].fecha_asignacion, token, {\cf17 sizeof}(descuentos[i].fecha_asignacion));\par
00346         token = strtok(NULL, {\cf22 "-"});\par
00347         strncpy(descuentos[i].fecha_caducidad, token, {\cf17 sizeof}(descuentos[i].fecha_caducidad));\par
00348         token = strtok(NULL, {\cf22 "-"});\par
00349         descuentos[i].estado = atoi(token);\par
00350         \}\par
00351     \}\par
00352 \par
00353     fclose(archivo);\par
00354     *numDescC = count;\par
00355     {\cf19 return} descuentos;\par
00356 \}\par
}
