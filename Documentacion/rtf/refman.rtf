00187         lockers[i].num_compt = atoi(token);\par
00188         token = strtok(NULL, {\cf22 "-"});\par
00189         lockers[i].num_compocup = atoi(token);\par
00190         \}\par
00191     \}\par
00192 \par
00193     fclose(archivo);\par
00194     *numLock = count;\par
00195     {\cf19 return} lockers;\par
